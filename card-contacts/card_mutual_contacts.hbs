<div class="user-card u_c">
	<div class="about-user">
			<a id="{{contact_user_id}}" data-loc="search" onclick="showContactProfile('{{contact_user_id}}')"><img class="mut_cont_prof" src="{{contact_profile_image}}" alt=""></a>
		<span class="user-info-detail">
			<a href="#" id="mutual-{{contact_user_id}}" data-loc="search" onclick="showContactProfile('{{contact_user_id}}')">{{contact_first_name}} {{contact_last_name}}</a><br>
			{{#if card_type_professional}}
				<strong>rating: <input id="rating-input" class="average-rating" value="{{rating}}" /></strong>
			{{else}}
				Rating NA
			{{/if}}
		</span>
	</div>
</div>
