{{#if landscape}}
<div class="card-box portrait" id="{{card_id}}" data-card-id="{{card_id}}" data-card-type={{card_type}} data-card-private={{card_private}} data-requires-reciprocity="{{requires_reciprocity}}">
{{else}}
<div class="card-box " id="{{card_id}}" data-card-id="{{card_id}}" data-card-type={{card_type}} data-card-private={{card_private}} data-requires-reciprocity="{{requires_reciprocity}}">
{{/if}}
	<div class="card-box-content">
		<div class="card-over-lay cradDetailLink">
			{{#if card_private}}
				{{#if landscape}}
					<img src="export/default/private_portrait.jpg" alt="">
				{{else}}
					<img src="export/default/private.jpg" alt="">
				{{/if}}
			{{else}}
				<img src="{{card_image_url}}?{{timestamp}}" alt="">
			{{/if}}
			<ul>
			{{#if card_private}}
					<li>Sorry, you can't see details of a private card.</li>
				{{else}}
					<li><a href="#" class="userDetailLink">View Card Details</a></li>
			{{/if}}
			</ul>
		</div>
		<div class="about-user">
			<a href="#" class="avatar-slide avsl search-avatar"><img src="{{profile_image}}" alt="" width="37" height="37" /></a>

			<div class="mutual-contacts mutual-contacts-slide">
				<div class="user-info-box">
					<h3>Mutual holders of this card</h3>
					<div class="scrolling-area scrollbar-style" id="m_c_{{card_id}}">

					</div>
				</div>
			</div>

			<span class="user-info-detail">
					<span class="full-name">{{full_name}}</span><br>
				{{#if card_type_professional}}
					{{#if rating_na}}
						<strong>Rating NA</strong>
					{{else}}	
						<strong>Avg rating: <input id="rating-input" class="average-rating" value="{{average_rating}}" /></strong>
					{{/if}}	
				{{else}}	
					<strong>Your rating: <input id="rating-input" class="average-rating" value="{{your_rating}}" /></strong>
				{{/if}}	
				<input name="checkboxG7" id="cbx-{{card_id}}" class="css-checkbox" type="checkbox">
				<label for="cbx-{{card_id}}" class="css-label"></label>
			</span>
		</div>
	</div>
</div>
